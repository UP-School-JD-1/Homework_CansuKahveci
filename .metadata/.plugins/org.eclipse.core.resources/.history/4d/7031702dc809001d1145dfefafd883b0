package factorial;

public class Factorial {

	public static void main(String[] args) {
		Factorial factorial = new Factorial();
		System.out.print(factorial.CalculateFactorial(5));
	}

	int factorial = 0;

	public int CalculateFactorial(int n) {

		if (n == 1 | n == 0) {
			factorial = 1;
		} else {
			factorial = factorial * n;
		}

		return factorial;
	}

}
